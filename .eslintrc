{
    "extends":"airbnb",
    "parserOptions":{
        "ecmaVersion":6
    },
    "env":{
        "node":true,
        "browser":true,
        "es6":true
    },
    "rules":{
        "comma-dangle":"off",
        "react/prefer-stateless-function":"off",
        "no-unused-vars":[
            "error",
            {
                "vars":"local",
                "args":"none"
            }
        ],
        "react/jsx-filename-extension":"off"
    }
}